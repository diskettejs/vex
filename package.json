{
  "name": "@diskette/vex",
  "version": "0.7.1",
  "description": "Compile vanilla-extract .css.ts files into CSS, JS, and TypeScript declarations",
  "keywords": [
    "vanilla-extract",
    "css",
    "css-in-js",
    "typescript",
    "compiler",
    "build-tool"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules tsconfig.tsbuildinfo",
    "prepublishOnly": "pnpm build",
    "release": "changeset version && changeset publish && git push --follow-tags"
  },
  "exports": {
    ".": {
      "@repo/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "vex": "./dist/cli.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@vanilla-extract/css": "^1"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.7",
    "@vanilla-extract/integration": "8.0.6",
    "chalk": "^5.6.2",
    "citty": "^0.1.6",
    "esbuild": "^0.27.0",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@vanilla-extract/css": "^1.17.5",
    "prettier": "^3.4.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  },
  "packageManager": "pnpm@10.22.0",
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
