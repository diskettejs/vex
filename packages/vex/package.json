{
  "name": "@diskette/vex",
  "version": "0.16.0",
  "description": "Compiles vanilla-extract .css.ts files into CSS, JS, and TypeScript declarations",
  "keywords": [
    "vanilla-extract",
    "css",
    "css-in-js",
    "typescript",
    "compiler",
    "build-tool"
  ],
  "type": "module",
  "scripts": {
    "dev": "tsc -p tsconfig.build.json --watch",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules tsconfig.tsbuildinfo"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "vex": "./dist/cli.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@vanilla-extract/css": "^1"
  },
  "dependencies": {
    "@repeaterjs/repeater": "^3.0.6",
    "@vanilla-extract/integration": "8.0.6",
    "chalk": "^5.6.2",
    "citty": "^0.1.6",
    "log-update": "^7.0.2",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "@vanilla-extract/css": "^1.17.5",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diskettejs/vex.git"
  }
}
